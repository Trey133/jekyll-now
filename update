#!/usr/bin/env python
import subprocess
while True:
    gitadd = subprocess.check_output(["git", "add", "-A"])
    commit = subprocess.check_output(["git", "commit", "-m", "Update"])
    push = subprocess.check_output(["git", "push", "origin"])
    build_site = subprocess.check_output(["jekyll" "build"])
    print "%s\n" % str(gitadd)
    print "%s\n" % str(commit)
    print "%s\n" % str(push)
    print "%s\n" % str(build_site)
    time.sleep(10)
